#include "holberton.h"


/**
 * *_strstr - gets the length of a prefix substring.
 * @haystack: variable
 * @needle: variable
 * main - Return
 * Return: void
 */

char *_strstr(char *haystack, char *needle)
{
int contador;
contador = 0;
while (s[contador] != '\0')
{
if (*(accept) == *(s + contador))
{
return (&s[contador]);
}
contador++;
}
return (NULL);
}
