#include "holberton.h"


/**
 * *_strncpy - concatenates two strings.
 * @dest: variable
 * @src: variable
 * @n: variable
 * main - Return
 * Return: void
 */

char *_strncpy(char *dest, char *src, int n)
{
int i;
i = 0;
while (src[i] && i < n)
{
dest[i] = src[i];
i++;
}
while(dest[i])
{
  i++;
}
dest[i] = '\0';
return (dest);
}
