#include "holberton.h"


/**
 * *_strcat - concatenate string
 * @dest: variable
 * @src: variable
 * main - Return
 * Return: void
 */

char *_strcat(char *dest, char *src)
{
int i, j;
i = 0;
j = 0;
while (dest[i] != 0)
{
i++;
}
while (src[j] != 0)
{
dest[i + j] = src[j],
j++;
}
return (dest + src);
}
